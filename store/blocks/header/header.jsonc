  //============== Order Configuration Starts =============

  "flex-layout.col#orderConfig": {
    "children": ["order-config#header"],
    "props": {
      "blockClass": "orderConfig"
    }
  },

  "order-config#header": {
    "children": ["order-config.title#header", "order-config.modal#header"],
    "props": {
      "showInTitle": false,
      "formFields": [
        {
          "name": "orderType",
          "type": "string",
          "fieldType": "radio",
          "label": "Order Type",
          "defaultValue": "resale",
          "options": [
            {
              "label": "Resale",
              "value": "resale"
            },
            {
              "label": "Consumption",
              "value": "consumption"
            }
          ]
        }


      ]
    }
  },

  "order-config.title#header": {
    "props": {
      "formTitle": "Order Configuration",
      "blockClass": "orderConfig-title"
    }
  },

  "order-config.modal#header": {
    "children": ["order-config.form"]

  },

  "order-config.form":{
    "children":[
         "orderConfigForm"
         ]
  },

  //========== Order Configuration Ends============



  "header-layout.mobile": {
    "children": ["sticky-layout#mobile"]
  },

  "sticky-layout#mobile": {
    "children": ["flex-layout.row#mobileHeader"],
    "props": {
      "blockClass": "headerMobile"
    }
  },

  "flex-layout.row#mobileHeader": {
    "children": ["flex-layout.col#mobileHeader"]
  },

  "slider-layout#menuLinks": {
    "props": {
      "itemsPerPage": {
        "phone": 2
      },
      "centerMode": "to-the-left",
      "infinite": false,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "stripeSlider",
      "fullWidth": true
    },
    "children": [
      "main-menu#Demo",
      "rich-text#headStripe1",
      "rich-text#headStripe2",
      "rich-text#headStripe3",
      "rich-text#headStripe4",
      "rich-text#headStripe5"
    ]
  },

  "flex-layout.col#mobileHeader": {
    "children": ["flex-layout.row#mobile"],
    "props": {
      "blockClass": "main-header-mobile"
    }
  },

  "flex-layout.row#mobile": {
    "children": ["drawer","drawer", "logo#desktop", "quick-order-link", "minicart.v2"],
    "props": {
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "colSizing": "auto",
      "fullWidth": true
    }
  },

