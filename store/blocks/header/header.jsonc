{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },

  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },

  "header-layout.desktop": {
    "children": ["sticky-layout#desktop"]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header-pierce"
    },
    "children": [
      "notification.bar#header",
      "flex-layout.row#desktop1",
      "flex-layout.row#reactHeader"
      // "flex-layout.row#desktop2"
    ]
  },
  "flex-layout.row#reactHeader":{
"props":{
"blockClass":"pierce-reactHeader",
"fullWidth":true
},
"children":[
  "pierce-header"
]
  },
  "notification.bar#header":{
"props":{
  "content":"Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliquaâ€¦ Learn More",
  "blockClass":"topbarHeader"

}
  },
  "flex-layout.row#desktop1": {
    "props": {
      "blockClass": "header-pierce",
      "colGap": 4,
      "colSizing": "auto",
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    },
    "children": [
      "logo#desktop",
      "rich-text#pierceHeader",
      "vtex.menu@2.x:menu#main-menu-static",
      "flex-layout.col#spacer",
      "login",
      "quick-order-link",      //React Component
      "minicart.v2",
      "rich-text#cartTotal"  //static for demo
      // "rich-text#jobsFromCart"
    ]
  },
  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/icons/pierce-logo.png",
      "blockClass":"pierce-logo"
    }
  },
  "rich-text#pierceHeader":{
"props":{
  "text":"PARTS & SERVICE",
  "blockClass":"pierceHeader"
}
  },
  "vtex.menu@2.x:menu#main-menu-static": {
    "props": {
      "experimentalOptimizeRendering": true,
      "blockClass":"pierce-menu-container"
    },
    "children": [
      "menu-item#resources",
      "menu-item#customerSupport"
    ]
  },
  "menu-item#resources": {
    "props": {
      "id": "resources",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/resources",
        "noFollow": false,
        "tagTitle": "resources",
        "text": "Resources"
      },
      "iconProps": {
        "id": "nav-caret--down",
        "size": 12,
        "viewBox": "0 0 16 16",
        "activeClassName": "black",
        "mutedClassName": "c-action-primary"
      },
      "iconPosition": "right",

      "blockClass": "pierce-menu-link"
    }
  },
  "menu-item#customerSupport": {
    "props": {
      "id": "customer-support",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/customer-support",
        "noFollow": false,
        "tagTitle": "customer support",
        "text": "Customer Support"
      },
      "iconProps": {
        "id": "nav-caret--down",
        "size": 12,
        "viewBox": "0 0 16 16",
        "activeClassName": "black",
        "mutedClassName": "c-action-primary"
      },
      "iconPosition": "right",

      "blockClass": "pierce-menu-link"
    }
  },
  "login": {
    "props": {
      "iconLabel": "Hello, Sign in",
     "showIconProfile":true,
     "blockClass":"pierce-login"
    }
  },
  "rich-text#cartTotal":{
"props":{
  "text":"Cart Total",
  "textColor":"c-on-emphasis",
  "blockClass":"cartTotalText"
}
  },
  "rich-text#jobsFromCart":{
    "props":{
      "text":"items From This Job",
      "textColor":"c-on-emphasis"
    }
      },
// ==============ROW2======================
  "flex-layout.row#desktop2": {
    "props": {
      "blockClass": "header-pierce-row2",
      "colGap": 4,
      "colSizing": "auto",
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
    },
    "children": [
     "drawer",
     "flex-layout.row#searchBarMyFleet"
    //  "parallelogram-shape"
    ]
  },

  "flex-layout.row#searchBarMyFleet":{
    "props":{
      "colGap":4,
      "width":"grow",
      "blockClass":"searchBarMyFleet",
      "preventHorizontalStretch":true,
      "preventVerticalStretch":true
      },
      "children":[
        "image#dashboard",
        "link#headerDashboard",
        "myFleetUI"

      ]
  },

  "image#dashboard": {
    "props": {
      "src": "assets/icons/home-icon.png",
      "maxHeight": 25,
      "blockClass":"pierce-home-icon"
    }
  },
  "link#headerDashboard": {
    "props": {
      "label": "DASHBOARD",
      "href": "/dashboard",
      "blockClass": "header-dashboard"
    }
  },
  // ======================= Old theme codes============================
  "flex-layout.col#search-bar": {
    "children": ["search-bar#header"],
    "props": {
      "paddingLeft": 8,
      "width": "grow"
    }
  },

  "search-bar#desktop":{
    "props": {
      "openAutocompleteOnFocus": true,
      "blockClass": "searchMobile"
    }
  },

  "flex-layout.row#headerStripe": {
    "props": {
      "blockClass": "headerStripe",
      "horizontalAlign": "left",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    },
    "children": [
      "shopper-location",
      "main-menu#Demo",
      "rich-text#headStripe1",
      "rich-text#headStripe2",
      "rich-text#headStripe3",
      "rich-text#headStripe4",
      "rich-text#headStripe5",
      "flex-layout.col#orderConfig",
      "flex-layout.col#spacer",
      "check-permission#reseller",
      "localeSwitcherApp"

    ]
  },


