{
  "store.home": {
    "blocks": ["responsive-layout.desktop", "responsive-layout.mobile"]
  },
  "responsive-layout.desktop": {
    "children": [
      "notification#header",
      "notification#promotions",
      "check-permission#promotion",
      "flex-layout.row#imageList",
      "__fold__.experimentalLazyAssets",
      "flex-layout.row#shopByCategory",
      "__fold__.desktop",
      // Deprecated
      // "flex-layout.row#shopByBrand",
      "flex-layout.row#shelf1",
      "flex-layout.row#imageBanners",
      "flex-layout.row#singleBanner",
      "flex-layout.row#popular",
      "flex-layout.row#trending",
      "flex-layout.row#recommendation-shelf",
      "flex-layout.row#recommendation-refresh",
      "flex-layout.row#benefits",
      "flex-layout.row#newsletter"
    ],
       "parent": {
           "challenge": "challenge.trade-policy-condition"
         }
  },
  "notification#promotions":{
    "props":{
      "type": "success",
      "title": "Get Up To",
      "message": "25% Off."
    }
  },
  "notification#header":{
    "props":{
      "type": "header",
      "message": "FREE SHIPPING: Applies to USA orders over $2000+ | excluding HI, PR, AK, and CAN."
    }
  },
  "check-permission#promotion": {
    "props": {
      "roles": [
        "store-admin",
        "sales-admin"
      ]
    },
    "blocks": [
      "allowed-content#promotion",
      "disallowed-content#promotion"
    ]
  },
  "allowed-content#promotion": {
    "children": [
      "promotion#allowed"
    ]
  },
  "disallowed-content#promotion": {
    "children": [
      "promotion#disallowed"
    ]
  },
  "promotion#allowed": {
    "props": {
      "heading": "Congratulations !!",
      "subHeading": "Special offer unlocked for you : Get 50% OFF on shopping above 15000$"
    }
  },
  "promotion#disallowed": {
    "props": {
      "heading": "Hello There !!",
      "subHeading": "Please login to see exclusive offers and shipping charges"
    }
  },
  "challenge.trade-policy-condition": {
    "props": {
      "redirectPath": "/login"
    }
  },
  // ========Recommendation shelf===========
  "flex-layout.row#recommendation-shelf": {
    "children": ["recommendation-shelf"]
  },
  "recommendation-shelf": {
    "blocks": ["default-shelf"],
    "props": {
      "strategy": "NAVIGATION_HISTORY",
      "secondaryStrategy": "MOST_POPULAR",
      "recommendation": {
        "count": {
          "minimum": 5,
          "recommendations": 20
        }
      }
    }
  },
  "flex-layout.row#recommendation-refresh": {
    "children": ["recommendation-refresh"]
  },
  "recommendation-refresh": {
    "blocks": ["refresh-shelf"],
    "props": {
      "strategy": "ORDER_HISTORY",
      "secondaryStrategy": "BEST_SELLERS",
      "recommendation": {
        "count": {
          "minimum": 5,
          "recommendations": 20
        }
      }
    }
  },
  // ========================================
  "flex-layout.row#imageList": {
    "props": {
      "blockClass": "full-banner"
    },
    "children": ["list-context.image-list#homeBanners"]
  },
  "flex-layout.row#imageList--mobile": {
    "props": {
      "blockClass": ["full-banner", "full-banner--mobile"],
      "fullWidth": true
    },
    "children": ["list-context.image-list#homeBanners"]
  },
  "list-context.image-list#homeBanners": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 328,
      "images": [
        {
          "width": "1440px",
          "image": "assets/placeholders/fullbannerDesktop.png",
          "mobileImage": "assets/placeholders/fullbannerMobile.png",
          "description": "something something"
        },
        {
          "width": "1440px",
          "image": "assets/placeholders/fullbannerDesktop.png",
          "mobileImage": "assets/placeholders/fullbannerMobile.png",
          "description": "something something"
        }
      ]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "blockClass": ["homeSlider", "dotsCustom"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": false,
      "showNavigationArrows": "desktopOnly"
    }
  },
  "flex-layout.row#shopByCategory": {
    "props": {
      "blockClass": "shopByCategorySection"
    },
    "children": ["flex-layout.col#shopByCategory"]
  },
  "flex-layout.col#shopByCategory": {
    "props": {
      "blockClass": "shopByCategory"
    },
    "children": ["rich-text#shopByTitle", "flex-layout.row#categories"]
  },
  "rich-text#shopByTitle": {
    "props": {
      "text": "### Shop by category",
      "blockClass": "shopByCategoryTitle"
    }
  },
  "flex-layout.row#categories": {
    "props": {
      "blockClass": "shopByCategory",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "link#shoBy1",
      "link#shoBy2",
      "link#shoBy3",
      "link#shoBy4",
      "link#shoBy5",
      "link#shoBy6"
    ]
  },
  "link#shoBy1": {
    "children": ["image#shoBy1", "rich-text#cat1"],
    "props": {
      "href": "/merchandise",
      "blockClass": "shopByLink"
    }
  },
  "link#shoBy2": {
    "children": ["image#shoBy2", "rich-text#cat2"],
    "props": {
      "href": "/nismo",
      "blockClass": "shopByLink"
    }
  },
  "link#shoBy3": {
    "children": ["image#shoBy3", "rich-text#cat3"],
    "props": {
      "href": "/car-parts",
      "blockClass": "shopByLink"
    }
  },
  "link#shoBy4": {
    "children": ["image#shoBy4", "rich-text#cat4"],
    "props": {
      "href": "/major-appliances",
      "blockClass": "shopByLink"
    }
  },
  "link#shoBy5": {
    "children": ["image#shoBy5", "rich-text#cat5"],
    "props": {
      "href": "/small-appliances",
      "blockClass": "shopByLink"
    }
  },
  "link#shoBy6": {
    "children": ["image#shoBy6", "rich-text#cat6"],
    "props": {
      "href": "/electronics",
      "blockClass": "shopByLink"
    }
  },
  "image#shoBy1": {
    "props": {
      "src": "https://dulick.nissan.ie/sites/default/files/home_promo_images/NISMO021.jpg",
      "blockClass": "shoByImage"
    }
  },
  "image#shoBy2": {
    "props": {
      "src": "https://img.joomcdn.net/1039cf936a38448e7f9b96879f73cc1a6b4d4993_original.jpeg",
